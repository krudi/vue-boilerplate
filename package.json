    {
      "type": "MIT",
      "url": "https://github.com/krudi/vue-template/LICENSE"
    }
  ],
  "version": "0.1.0",
  "publisher": "author",
  "authors": [
    {
      "name": "authors",
      "email": "example@mail.com",
      "url": "https://github.com/krudi/vue-template"
    }
  ],
  "private": false,
  "homepage": "https://github.com/krudi/vue-template#readme",
  "bugs": {
    "url": "https://github.com/krudi/vue-template/issues"
  },
  "keywords": [
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krudi/vue-template.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "http://example.com/donate"
    }
  ],
  "engines": {
    "node": ">= 18.17.1"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.vue .",
    "lint:css": "stylelint **/*.{css,scss}",
    "lint:css:fix": "stylelint --fix **/*.{css,scss}",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:markdown": "markdownlint **/*.md",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files && npm run lint:markdown",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files:fix && npm run lint:markdown",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "cy:e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "cy:component": "cypress open --component",
    "cy:component:headless": "cypress run --component",
    "jest:run": "jest",
    "deploy": "npm run generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxt/cli-edge": "^2.17.2-28179381.14bb6c2",
    "@nuxt/typescript-runtime": "^3.0.1",
    "esbuild": "^0.18.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nuxt/devtools": "^0.8.0",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/types": "^2.17.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.6.2",
    "cssnano": "^6.0.1",
    "cypress": "^12.17.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.2.3",
    "markdownlint": "^0.29.0",
    "markdownlint-cli": "^0.35.0",
    "nuxt": "^3.6.5",
    "postcss": "^8.4.27",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "vue-tsc": "^1.8.8"
  }
}
