    "eslint-plugin-vue": "^9.23.0",
    "nuxt": "^3.11.0",
    "postcss": "^8.4.36",
    "postcss-custom-media": "^10.0.6",
    "postcss-import": "^16.0.0",
    "postcss-load-config": "^5.0.2",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.4.5",
    "vue-tsc": "^2.0.19"
  }
