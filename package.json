  },
  "funding": [
    {
      "type": "individual",
      "url": "http://example.com/donate"
    }
  ],
  "engines": {
    "node": ">= 20.11.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.vue .",
    "lint:css": "stylelint **/*.{css,scss}",
    "lint:css:fix": "stylelint --fix **/*.{css,scss}",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files:fix",
    "deploy": "npm run generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxt/cli-edge": "^2.17.2-28284499.5674f49",
    "@nuxt/typescript-runtime": "^3.0.2",
    "esbuild": "^0.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/types": "^2.17.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.6.5",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-scss": "^4.0.7",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.8"
  }
}
