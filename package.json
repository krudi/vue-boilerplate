  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.vue .",
    "lint:css": "stylelint **/*.{css,scss}",
    "lint:css:fix": "stylelint --fix **/*.{css,scss}",
    "lint:files": "prettier --check \"**\"",
    "lint:files:fix": "prettier --write \"**\"",
    "lint:markdown": "markdownlint **/*.md",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files && npm run lint:markdown",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files:fix && npm run lint:markdown",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "cy:e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "cy:component": "cypress open --component",
    "cy:component:headless": "cypress run --component",
    "jest:run": "jest",
    "deploy": "npm run generate"
  },
  "dependencies": {
    "@nuxt/cli-edge": "^2.17.1-28105940.1ce34ec",
