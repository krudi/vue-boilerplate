{
  "name": "vue-template",
  "description": "A template with Nuxt3 built on Vue3 with focus on performance and best practices.",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/krudi/vue-template/LICENSE"
    }
  ],
  "version": "0.1.0",
  "authors": [
    {
      "name": "krudi",
      "email": "patryk.kudlik@gmail.com",
      "url": "https://github.com/krudi/"
    }
  ],
  "private": false,
  "homepage": "https://github.com/krudi/vue-template#readme",
  "bugs": {
    "url": "https://github.com/krudi/vue-template/issues"
  },
  "keywords": [
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krudi/vue-template.git"
  },
  "engines": {
    "node": ">= 20.11.0",
    "npm": ">= 10.8.1"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "lint:js": "eslint --ext .js,.ts,.vue .",
    "lint:js:fix": "eslint --fix --ext .js,.ts,.vue .",
    "lint:css": "stylelint **/*.css",
    "lint:css:fix": "stylelint --fix **/*.css",
    "lint:files": "prettier --check \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:files:fix": "prettier --write \"**/*.{html,graphql,yml,yaml,vue}\"",
    "lint:full": "npm run lint:js && npm run lint:css && npm run lint:files",
    "lint:full:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:files:fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.3.2",
    "@nuxt/eslint-config": "0.3.13",
    "@nuxt/types": "2.17.3",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@types/node": "20.12.13",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "@vite-pwa/nuxt": "^0.8.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.26.0",
    "nuxt": "3.11.2",
    "postcss": "8.4.38",
    "postcss-custom-media": "10.0.6",
    "postcss-import": "16.1.0",
    "postcss-load-config": "5.1.0",
    "postcss-nested": "6.0.1",
    "prettier": "3.2.5",
    "stylelint": "16.6.1",
    "stylelint-config-standard": "36.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vue-tsc": "1.8.27"
  }
}
